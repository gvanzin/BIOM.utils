#-----------------------------------------------------------------------------
#  these routines are available to users:
#-----------------------------------------------------------------------------

export(
	str.biom,				# basic class utilites
	summary.biom,
	print.biomsummary,
	print.biom,
	dim.biom,
	dimnames.biom,

	metadata.biom,
	
	as.matrix.biom,			# conversion from
	as.list.biom,
	as.character.biom,

	biom,					# conversion to
	biom.character,
	biom.matrix,
	biom.list,

	buildBiomExamples,		# package utilities
	applyBiomMethods,
	exampleBiomFile)

#-----------------------------------------------------------------------------
#  these S3 methods are defined:
#-----------------------------------------------------------------------------

S3method (as.character, biom)
S3method (as.matrix, biom)
S3method (dim, biom)
S3method (dimnames, biom)
S3method (print, biom)
S3method (str, biom)
S3method (summary, biom)
S3method (biom, list)
S3method (biom, matrix)
S3method (biom, character)
S3method (print, biomsummary)
S3method (metadata, biom)

